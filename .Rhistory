individual_parameters <- function(id_n, rfg, fit){
if(unique(rfg)==0){
par <- c(mean(extract_rstan(fit, "par", 1)), mean(extract_rstan(fit, "par", 2)))
}else{
par <- c(mean(extract_rstan(fit, "par", 1)+extract_rstan(fit, "d_rfg", 1)), mean(extract_rstan(fit, "par", 2)+extract_rstan(fit, "d_rfg", 2)))
}
U1_i <- mean(extract_rstan(fit, "u", 1, unique(id_n)))
U2_i <- mean(extract_rstan(fit, "u", 2, unique(id_n)))
return(list(mu = par[1] + U1_i,
sigma = exp(par[2] + U2_i)))
}
d$bias <- NA; d$sigma <- NA;
for(i in 1:nrow(d)){
par_i <- individual_parameters(d$id_n[i], d$refugee[i], fit)
d$bias[i] <- par_i$mu
d$sigma[i] <- par_i$sigma
}
